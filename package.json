{
  "name": "next-technology-website",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf node_modules && rm -rf .next",
    "type-check": "tsc --pretty --noEmit",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier './**/*.{css,js,mdx}' --write",
    "check-changed-files": "ts-node scripts/ts-check-changed-files.ts"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",
    "cva": "npm:class-variance-authority",
    "final-form": "^4.20.9",
    "next": "13.5.4",
    "next-international": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "server-only": "^0.0.1",
    "tailwind-merge": "^1.14.0",
    "typed.js": "^2.0.16",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@vercel/style-guide": "^5.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.5.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.24",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  }
